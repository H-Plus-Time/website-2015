<script>
    // build the mini location squares
    $.get("./data/locations.json", function(data) {
        $.each (Object.keys(data), function(index, city) {
            var $loc_col_div = $("<div>", { class: "col-sm-3" });
            var $loc_href = $("<a>", { href: data[city]["page"], class: "event-region" });
            var $loc_city_image = $("<img>", { src: data[city]["image-sm"], class: "event-region-image" });
            var $loc_shad_span = $("<span>", { class: "event-region-shadow" });
            var $loc_text_span = $("<span>", { class: "event-region-text" });
            $loc_text_span.append($("<h2>", { html: city, class: "font-light" }));
            $loc_text_span.append($("<p>", { html: data[city]["where_readable"][0] }));
            $loc_text_span.append($("<div>", { html: "Learn More", class: "btn-link" }));
            $loc_href.append($loc_city_image);
            $loc_href.append($loc_shad_span);
            $loc_href.append($loc_text_span);
            $loc_col_div.append($loc_href);
            $("#event-regions").append($loc_col_div);
        });
    });

    $(window).load(function() {
        equalheight('.row .top-container-col');
    });

    $(window).resize(function(){
        equalheight('.row .top-container-col');
    });

</script>
